@page
@model IndexModel
@{ ViewData["Title"] = "Home page"; }

<div class="container">
    <div class='row'>
        @{ 
            
            foreach (var item in Model.Users)
            {
    <div class="card" style="width: 18rem;">
        <img class="card-img-top" src="/uploads/@item.ImagePath" alt="@item.UserName">
        <div class="card-body">
            <h5 class="card-title">@item.FirstName</h5>
            <p class="card-text">@item.About</p>
            <a class="btn btn-primary" href="/user/@item.UserName">Visit profile</a>
        </div>
    </div> } }

    </div>
</div>
<p>
    @ViewBag.userid
</p>